{
  "name": "threejs-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "TheFrost <frost084@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rm -rf .cache/ && parcel src/index.html --no-source-maps -d public/ --open",
    "build": "rm -rf build/ && parcel build src/index.html --no-source-maps -d build/ && cp -R public/resources build/resources"
  },
  "dependencies": {
    "three": "^0.97.0",
    "three-obj-mtl-loader": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "json5": "^2.1.0",
    "parcel-bundler": "^1.10.1"
  }
}
